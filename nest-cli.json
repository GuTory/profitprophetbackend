{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "outdir": "./dist",
    "rootDir": "./",
    "deleteOutDir": true
  },
  "include": ["src/**/*.ts", "python/", "data/"],
  "exclude": ["node_modules", "dist"]
}
